<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <title>BBS</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  </head>
  <body>
    <nav class="navbar navbar-inverse ">
      <div class="navbar-header">
        <a class="navbar-brand" href="./bbs">BBS</a>
      </div>
    </nav>
    <div class="form-inline form-group center-block">
      <form method="post">
        <input class="form-control form-warning" type="text" name="maintext" placeholder="テキストを入力">
        <input class="btn btn-primary" type="submit" value="投稿">
      </form>
    </div>
    <!--
    <ul>
      <% for(var i in send_data){ %>
        <li><%= send_data[i].maintext %> - <%= send_data[i].created %></li>
      <% } %>
    </ul>
    -->
    <table class="table table-striped table-bordered table-hover">
      <tr>
        <th>テキスト</th>
        <th>日時</th>
      </tr>
      <% for(var i in send_data){ %>
        <tr>
          <td><%= send_data[i].maintext %></td>
          <td><%= send_data[i].created %></td>
        </tr>
      <% } %>
    </table>
  </body>
</html>
