<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <title>Neurnect</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pixi.js/4.0.0/pixi.min.js"></script>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.11.2/css/bootstrap-select.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.11.2/js/bootstrap-select.min.js"></script>
    <script src="https://cdn.socket.io/socket.io-1.4.8.js"></script>
    <link rel="stylesheet" href="/css/app.css">
  </head>
  <body>
    <!-- ナビゲーションバー -->
    <nav class="navbar navbar-inverse">
      <div class="navbar-header">
        <a class="navbar-brand" href="/">Neurnect</a>
      </div>
    </nav>

    <!-- ボタン各種 -->
    <div class="center-block">
      <a class="btn btn-primary" id="reload"><span class="glyphicon glyphicon-refresh"></span></a>
      <a class="btn btn-primary" id="post"><span class="glyphicon glyphicon-pencil"></span></a>
    </div>

  <!-- カテゴリテスト -->
  <div class"center-blocl">
    <a class="btn btn-primary" id="category-test">Test</a>
  </div>

    <!-- 投稿フォーム -->
    <div class="form col-sm-offset-3 col-sm-6">
      <a class="btn btn-default pull-right close" id="form_remove"><span class="glyphicon glyphicon-remove-circle"></span></a>
        <form>
          <div class="form-group">
            <label class="control-label">Form:</label>
            <select class="selectpicker" id="graphic-form">
              <option>ellipse</option>
              <option>rect</option>
            </select>
          </div>
          <div class="col-sm-6 form-group">
            <label class="control-label">Text:</label>
            <span class="control-label" id="uploadtext-limit">100</span>
            <textarea class="form-control" id="uploadtext" maxlength="100" placeholder="メインテキスト"></textarea>
          </div>
          <div class="col-sm-6 form-group">
            <label class="control-label">Tag:</label>
            <input type="tag" class="form-control" id="tag-select" placeholder="タグ">
          </div>
          <div class="col-sm-6 form-group">
            <a class="btn btn-primary form-control" id="submit">送信</a>
          </div>
        </form>
    </div>

    <!-- 描画コンテナ -->
    <div class="col-sm-12" id="container"></div>

    <script type="text/ecmascript" src="/js/model.normal.js"></script>
    <script type="text/ecmascript" src="/js/view.field.js"></script>
    <script type="text/ecmascript" src="/js/view.normal.object.js"></script>
    <script type="text/ecmascript" src="/js/model.normal.tag.js"></script>
    <script type="text/ecmascript" src="/js/model.category.js"></script>
    <script type="text/ecmascript" src="/js/model.conversation.js"></script>
    <script type="text/ecmascript" src="/js/view.conversation.js"></script>
    <script type="text/ecmascript" src="/js/typical.js"></script>
    <script type="text/ecmascript" src="/js/event.js"></script>
    <script type="text/ecmascript" src="/js/main.js"></script>
  </body>
</html>
